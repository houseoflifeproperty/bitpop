[
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "config",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'http://v8.googlecode.com/svn/branches/bleeding_edge'}]"
    ],
    "name": "gclient setup"
  },
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "sync",
      "--nohooks",
      "--delete_unversioned_trees",
      "--force",
      "--verbose",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "name": "gclient sync",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"solutions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"v8/\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": 50110@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@50110@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "env": {
      "GYP_DEFINES": "target_arch=ia32 v8_optimized_debug=1 v8_target_arch=ia32"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/cleanup_temp.py"
    ],
    "name": "cleanup_temp"
  },
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/compile.py",
      "--target",
      "Debug",
      "--src-dir",
      "[SLAVE_BUILD]/v8",
      "--build-tool",
      "xcode",
      "--"
    ],
    "name": "compile"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Debug",
      "--arch",
      "ia32",
      "--testname",
      "mjsunit",
      "--gc-stress",
      "--shard_count=3",
      "--shard_run=3",
      "--flaky-tests",
      "skip"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Mjsunit",
    "seed_steps": [
      "Mjsunit",
      "Mjsunit - flaky",
      "Webkit",
      "Webkit - flaky"
    ]
  },
  {
    "abort_on_failure": false,
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Debug",
      "--arch",
      "ia32",
      "--testname",
      "mjsunit",
      "--gc-stress",
      "--shard_count=3",
      "--shard_run=3",
      "--flaky-tests",
      "run"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Mjsunit - flaky"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Debug",
      "--arch",
      "ia32",
      "--testname",
      "webkit",
      "--gc-stress",
      "--shard_count=3",
      "--shard_run=3",
      "--flaky-tests",
      "skip"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Webkit"
  },
  {
    "abort_on_failure": false,
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Debug",
      "--arch",
      "ia32",
      "--testname",
      "webkit",
      "--gc-stress",
      "--shard_count=3",
      "--shard_run=3",
      "--flaky-tests",
      "run"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Webkit - flaky"
  }
]