  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "env": {
      "GYP_CHROMIUM_NO_ACTION": "1"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gn.py",
      "--root=[SLAVE_BUILD]/src",
      "gen",
      "//out/Debug",
      "--args=is_debug=true os=\"android\" cpu_arch=\"arm\" use_goma=true goma_dir=\"[BUILD]/goma\""
    ],
    "name": "gn"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/compile.py",
      "--target",
      "Debug",
      "--src-dir",
      "[SLAVE_BUILD]/src",
      "--build-tool",
      "ninja",
      "--compiler",
      "goma",
      "--goma-dir",
      "[BUILD]/goma",
      "--",
      "all"
    ],
    "name": "compile"
  }
